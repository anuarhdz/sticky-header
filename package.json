{
  "name": "quick-start-template",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "npm run clean && parcel src/index.html --dist-dir public -p 3000",
    "build": "npm run clean && parcel build src/index.html --dist-dir public",
    "clean": "rimraf ./public && rimraf ./.parcel-cache"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "autoprefixer": "^10.4.7",
    "parcel": "latest",
    "postcss": "^8.4.14",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.3"
  }
}
